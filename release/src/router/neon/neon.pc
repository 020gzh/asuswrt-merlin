prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: neon
Description: HTTP/WebDAV client library
Version: 0.29.6
Libs.private:  -lz -lssl -lcrypto   -L/usr/lib/x86_64-linux-gnu -lxml2
Libs: -L${libdir} -lneon 
Cflags: -I${includedir}/neon  -D_LARGEFILE64_SOURCE -DNE_LFS
